// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Projects {
  id              String @id @default(uuid())
  name            String
  img_url         String
  description     String
  time            String
  github_url      String
  url             String
  created_at      DateTime @default(now())
}

model Earns {
  id              String @id @default(uuid())
  typeid          Int
  quantity        Int
  type            String
  created_at      DateTime @default(now())
}

model Users {
  id              String @id @default(uuid())
  username        String
  password        String
  avatar_url      String
  role            Int
  created_at      DateTime @default(now())
}